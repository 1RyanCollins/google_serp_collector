{
  "manifest_version": 3,
  "name": "Google SERP Link Collector",
  "version": "1.0",
  "description": "Collects links from Google search results.",
  "permissions": ["activeTab", "scripting", "downloads"],
  "icons": {
    "48": "icons8-link-48.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons8-link-48.png"
    }
  }
}

